include ../../../img/svg/site.pug
mixin portfolio-screen__example-site-href(modifier)
  if modifier["exampleSiteHref"] == undefined
    - modifier["exampleSiteHref"] = "#!";
  a.portfolio-screen__example-site-href(href=modifier.exampleSiteHref)&attributes(attributes)
    +site(modifier)(class="portfolio-screen__example-href-img")